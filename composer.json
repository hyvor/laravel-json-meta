{
    "name": "hyvor/laravel-json-meta",
    "description": "Strict meta data for Eloquent Models, saved in JSON columns",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Supun Wimalasena",
            "email": "supun@hyvor.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hyvor\\JsonMeta\\": "src",
            "Hyvor\\JsonMeta\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.3|^8.2"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10",
        "pestphp/pest": "^2",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
